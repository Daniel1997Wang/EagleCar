<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="egearcom.egear.com.eaglecarui10.MainActivity">

    <LinearLayout
        android:layout_width="800dp"
        android:layout_height="480dp"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="430dp"
            android:layout_height="480dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="430dp"
                android:layout_height="80dp"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="230dp"
                    android:layout_height="80dp"
                    android:background="#FFFFFF"/>
                <TextView
                    android:layout_width="200dp"
                    android:layout_height="80dp"
                    android:background="#000000"/>

            </LinearLayout>

            <TextView
                android:layout_width="430dp"
                android:layout_height="20dp"
                android:background="#000000"/>

            <LinearLayout
                android:layout_width="430dp"
                android:layout_height="380dp"
                android:background="#000000"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="150dp"
                    android:layout_height="380dp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="130dp"
                        android:layout_height="190dp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="130dp"
                            android:layout_height="40dp"
                            android:text="Enginc Temperature"
                            android:textColor="#FFFFFF"
                            android:textSize="15dp"
                            android:gravity="center"/>

                        <LinearLayout
                            android:layout_width="130dp"
                            android:layout_height="150dp"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/enginc_temperature"
                                android:layout_width="90dp"
                                android:layout_height="150dp"
                                android:text="150"
                                android:textColor="#D50000"
                                android:textSize="50dp"
                                android:gravity="center"/>

                            <TextView
                                android:layout_width="40dp"
                                android:layout_height="150dp"
                                android:text="℃"
                                android:textColor="#FFFFFF"
                                android:textSize="25dp" />
                        </LinearLayout>

                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="130dp"
                        android:layout_height="190dp"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="130dp"
                            android:layout_height="40dp"
                            android:text="Oil Pressure"
                            android:textColor="#FFFFFF"
                            android:textSize="15dp"
                            android:gravity="center"/>

                        <LinearLayout
                            android:layout_width="130dp"
                            android:layout_height="150dp"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/oil_pressure"
                                android:layout_width="90dp"
                                android:layout_height="150dp"
                                android:text="888"
                                android:textColor="#FFFFFF"
                                android:textSize="50dp"
                                android:gravity="center"/>

                            <TextView
                                android:layout_width="40dp"
                                android:layout_height="150dp"
                                android:text="kPa"
                                android:textColor="#FFFFFF"
                                android:textSize="20dp" />
                        </LinearLayout>
                    </LinearLayout>




                </LinearLayout>

                <LinearLayout
                    android:layout_width="110dp"
                    android:layout_height="380dp"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="110dp"
                        android:layout_height="100dp"
                        android:background="#000000"/>

                    <TextView
                        android:layout_width="110dp"
                        android:layout_height="280dp"
                        android:text="2"
                        android:textSize="190dp"
                        android:textColor="#FFFFFF"
                        android:background="#000000"/>

                </LinearLayout>





                <LinearLayout
                    android:layout_width="170dp"
                    android:layout_height="380dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="170dp"
                        android:layout_height="65dp"
                        android:text="Exhaust"
                        android:textSize="33dp"
                        android:textColor="#D50000"
                        android:background="#FFFFFF"
                        android:gravity="center"/>
                    <TextView
                        android:layout_width="170dp"
                        android:layout_height="65dp"
                        android:text="Temperature"
                        android:textSize="28dp"
                        android:textColor="#D50000"
                        android:background="#FFFFFF"
                        android:gravity="center"/>

                    <TextView
                        android:layout_width="170dp"
                        android:layout_height="60dp"
                        android:background="#000000"/>

                    <TextView
                        android:id="@+id/exhaust_temperature_left"
                        android:layout_width="170dp"
                        android:layout_height="140dp"
                        android:text="888"
                        android:textSize="100dp"
                        android:textColor="#FFFFFF"/>
                    <TextView
                        android:layout_width="170dp"
                        android:layout_height="50dp"
                        android:text="Km/h"
                        android:textSize="20dp"
                        android:textColor="#FFFFFF"
                        android:gravity="right"/>

                </LinearLayout>









            </LinearLayout>



        </LinearLayout>


        <LinearLayout
            android:layout_width="370dp"
            android:layout_height="480dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="370dp"
                android:layout_height="140dp"
                android:orientation="horizontal"
                android:background="#000000">


                <LinearLayout
                    android:layout_width="123dp"
                    android:layout_height="140dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="123dp"
                        android:layout_height="20dp"
                        android:background="#000000"/>
                    <TextView
                        android:layout_width="123dp"
                        android:layout_height="40dp"
                        android:text="Oil Temperature"
                        android:textSize="15dp"
                        android:textColor="#FFFFFF"
                        android:gravity="center"/>
                    <LinearLayout
                        android:layout_width="123dp"
                        android:layout_height="80dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/oil_temperature"
                            android:layout_width="90dp"
                            android:layout_height="80dp"
                            android:text="888"
                            android:textSize="45dp"
                            android:textColor="#FFFFFF"
                            android:gravity="center"/>
                        <TextView
                            android:layout_width="33dp"
                            android:layout_height="80dp"
                            android:text="℃"
                            android:textSize="20dp"
                            android:textColor="#FFFFFF"/>
                    </LinearLayout>



                </LinearLayout>

                <LinearLayout
                    android:layout_width="123dp"
                    android:layout_height="140dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="123dp"
                        android:layout_height="20dp"
                        android:background="#000000"/>
                    <TextView
                        android:layout_width="123dp"
                        android:layout_height="40dp"
                        android:text="Air Temperature"
                        android:textSize="15dp"
                        android:textColor="#FFFFFF"
                        android:gravity="center"/>
                    <LinearLayout
                        android:layout_width="123dp"
                        android:layout_height="80dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/air_temperature"
                            android:layout_width="90dp"
                            android:layout_height="80dp"
                            android:text="88"
                            android:textSize="45dp"
                            android:textColor="#FFFFFF"
                            android:gravity="center"/>
                        <TextView
                            android:layout_width="33dp"
                            android:layout_height="80dp"
                            android:text="℃"
                            android:textSize="20dp"
                            android:textColor="#FFFFFF"/>
                    </LinearLayout>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="124dp"
                    android:layout_height="140dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="124dp"
                        android:layout_height="20dp"
                        android:background="#000000"/>
                    <TextView
                        android:layout_width="124dp"
                        android:layout_height="40dp"
                        android:text="Exhaust Temperature"
                        android:textSize="12dp"
                        android:textColor="#FFFFFF"
                        android:gravity="center"/>
                    <LinearLayout
                        android:layout_width="124dp"
                        android:layout_height="80dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/exhaust_temperature_right"
                            android:layout_width="90dp"
                            android:layout_height="80dp"
                            android:text="888"
                            android:textSize="45dp"
                            android:textColor="#D50000"
                            android:gravity="center"/>
                        <TextView
                            android:layout_width="34dp"
                            android:layout_height="80dp"
                            android:text="℃"
                            android:textSize="20dp"
                            android:textColor="#FFFFFF"/>
                    </LinearLayout>



                </LinearLayout>


            </LinearLayout>

            <TextView
                android:layout_width="370dp"
                android:layout_height="20dp"
                android:background="#78909C"/>



            <LinearLayout
                android:layout_width="370dp"
                android:layout_height="140dp"
                android:orientation="horizontal"
                android:background="#000000">


                <LinearLayout
                    android:layout_width="123dp"
                    android:layout_height="140dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="123dp"
                        android:layout_height="20dp"
                        android:background="#000000"/>
                    <TextView
                        android:layout_width="123dp"
                        android:layout_height="40dp"
                        android:text="Throttle Position"
                        android:textSize="15dp"
                        android:textColor="#FFFFFF"
                        android:gravity="center"/>
                    <LinearLayout
                        android:layout_width="123dp"
                        android:layout_height="80dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/throttle_position"
                            android:layout_width="90dp"
                            android:layout_height="80dp"
                            android:text="88.8"
                            android:textSize="45dp"
                            android:textColor="#FFFFFF"
                            android:gravity="center"/>
                        <TextView
                            android:layout_width="33dp"
                            android:layout_height="80dp"
                            android:text="%"
                            android:textSize="20dp"
                            android:textColor="#FFFFFF"/>
                    </LinearLayout>



                </LinearLayout>

                <LinearLayout
                    android:layout_width="123dp"
                    android:layout_height="140dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="123dp"
                        android:layout_height="20dp"
                        android:background="#000000"/>
                    <TextView
                        android:layout_width="123dp"
                        android:layout_height="40dp"
                        android:text="Manifold Pressure"
                        android:textSize="15dp"
                        android:textColor="#FFFFFF"
                        android:gravity="center"/>
                    <LinearLayout
                        android:layout_width="123dp"
                        android:layout_height="80dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/manifold_pressure"
                            android:layout_width="90dp"
                            android:layout_height="80dp"
                            android:text="88.8"
                            android:textSize="45dp"
                            android:textColor="#FFFFFF"
                            android:gravity="center"/>
                        <TextView
                            android:layout_width="33dp"
                            android:layout_height="80dp"
                            android:text="kPa"
                            android:textSize="18dp"
                            android:textColor="#FFFFFF"/>
                    </LinearLayout>



                </LinearLayout>

                <LinearLayout
                    android:layout_width="124dp"
                    android:layout_height="140dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="124dp"
                        android:layout_height="20dp"
                        android:background="#000000"/>
                    <TextView
                        android:layout_width="124dp"
                        android:layout_height="40dp"
                        android:text="Lambda"
                        android:textSize="20dp"
                        android:textColor="#FFFFFF"
                        android:gravity="center"/>
                    <TextView
                        android:id="@+id/lambda"
                        android:layout_width="124dp"
                        android:layout_height="80dp"
                        android:text="1.11"
                        android:textSize="45dp"
                        android:textColor="#FFFFFF"
                        android:gravity="center"/>



                </LinearLayout>


            </LinearLayout>

            <TextView
                android:layout_width="370dp"
                android:layout_height="20dp"
                android:background="#78909C"/>



            <LinearLayout
                android:layout_width="370dp"
                android:layout_height="140dp"
                android:orientation="horizontal"
                android:background="#000000">


                <LinearLayout
                    android:layout_width="123dp"
                    android:layout_height="140dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="123dp"
                        android:layout_height="20dp"
                        android:background="#000000"/>
                    <TextView
                        android:layout_width="123dp"
                        android:layout_height="40dp"
                        android:text="ECU Temperature"
                        android:textSize="15dp"
                        android:textColor="#FFFFFF"
                        android:gravity="center"/>
                    <LinearLayout
                        android:layout_width="123dp"
                        android:layout_height="80dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/ecu_temperature"
                            android:layout_width="90dp"
                            android:layout_height="80dp"
                            android:text="888"
                            android:textSize="45dp"
                            android:textColor="#FFFFFF"
                            android:gravity="center"/>
                        <TextView
                            android:layout_width="33dp"
                            android:layout_height="80dp"
                            android:text="℃"
                            android:textSize="20dp"
                            android:textColor="#FFFFFF"/>
                    </LinearLayout>



                </LinearLayout>

                <LinearLayout
                    android:layout_width="123dp"
                    android:layout_height="140dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="123dp"
                        android:layout_height="20dp"
                        android:background="#000000"/>
                    <TextView
                        android:layout_width="123dp"
                        android:layout_height="40dp"
                        android:text="Battery Voltage"
                        android:textSize="15dp"
                        android:textColor="#FFFFFF"
                        android:gravity="center"/>
                    <LinearLayout
                        android:layout_width="123dp"
                        android:layout_height="80dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/battery_voltage"
                            android:layout_width="90dp"
                            android:layout_height="80dp"
                            android:text="13.3"
                            android:textSize="45dp"
                            android:textColor="#FFFFFF"
                            android:gravity="center"/>
                        <TextView
                            android:layout_width="33dp"
                            android:layout_height="80dp"
                            android:text="V"
                            android:textSize="20dp"
                            android:textColor="#FFFFFF"/>
                    </LinearLayout>



                </LinearLayout>

                <LinearLayout
                    android:layout_width="124dp"
                    android:layout_height="140dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="124dp"
                        android:layout_height="20dp"
                        android:background="#000000"/>
                    <TextView
                        android:layout_width="124dp"
                        android:layout_height="40dp"
                        android:text="Bake"
                        android:textSize="15dp"
                        android:textColor="#FFFFFF"
                        android:gravity="center"/>
                    <LinearLayout
                        android:layout_width="124dp"
                        android:layout_height="80dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/bake"
                            android:layout_width="90dp"
                            android:layout_height="80dp"
                            android:text="10"
                            android:textSize="45dp"
                            android:textColor="#FFFFFF"
                            android:gravity="center"/>
                        <TextView
                            android:layout_width="34dp"
                            android:layout_height="80dp"
                            android:text="mpa"
                            android:textSize="15dp"
                            android:textColor="#FFFFFF"/>
                    </LinearLayout>

                </LinearLayout>


            </LinearLayout>

            <TextView
                android:layout_width="370dp"
                android:layout_height="20dp"
                android:background="#78909C"/>



        </LinearLayout>


    </LinearLayout>

</android.support.constraint.ConstraintLayout>
